  target 'MUXSDKStatsTests' do
    inherit! :search_paths
    # Pods for testing
#    pod 'Mux-Stats-Core', '~>3.5'
    pod 'Mux-Stats-Core', :git => 'https://github.com/muxinc/stats-sdk-objc.git', :branch => 'bmulligan/latency'
  end

end

